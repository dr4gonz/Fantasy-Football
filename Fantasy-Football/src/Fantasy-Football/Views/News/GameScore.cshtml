@model Fantasy_Football.Models.NflGame
<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered table-condensed">
            <thead>
                <tr>
                    <th>
                    </th>
                    <th>
                        Final
                    </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        @Model.AwayTeam
                    </td>
                    <td>
                        @Model.AwayScore
                    </td>
                </tr>
                <tr>
                    <td>
                        @Model.HomeTeam
                    </td>
                    <td>
                        @Model.HomeScore
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>